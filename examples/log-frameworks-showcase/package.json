{
  "name": "example-log-frameworks-showcase",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "start:koa": "tsx --watch koa/app.ts",
    "start:http": "tsx --watch node-http/app.ts",
    "start:express": "tsx --watch express/app.ts"
  },
  "dependencies": {
    "@neodx/log": "workspace:*",
    "express": "4.18.2",
    "koa": "2.14.2",
    "koa-router": "12.0.1"
  },
  "devDependencies": {
    "@types/express": "4.17.20",
    "@types/koa": "2.13.10",
    "@types/koa-router": "7.4.6",
    "tsx": "3.14.0",
    "typescript": "5.3.3"
  }
}
