{
  "name": "example-log-frameworks-showcase",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "start:koa": "tsx --watch koa/app.ts",
    "start:http": "tsx --watch node-http/app.ts",
    "start:express": "tsx --watch express/app.ts",
    "start:nest": "tsx --watch nestjs/app.ts"
  },
  "dependencies": {
    "@neodx/log": "workspace:*",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "express": "4.18.2",
    "koa": "2.14.2",
    "koa-router": "12.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/express": "4.17.18",
    "@types/koa": "2.13.9",
    "@types/koa-router": "7.4.5",
    "tsx": "3.13.0",
    "typescript": "5.2.2"
  }
}
