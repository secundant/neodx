{
  "name": "example-svg-vite",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.0.15",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@neodx/svg": "workspace:*",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-tsconfig-paths": "4.2.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "nuke": "rm -rf dist"
  },
  "nx": {
    "targets": {
      "dev": {
        "dependsOn": [
          "^build"
        ]
      },
      "build": {
        "outputs": [
          "{workspaceRoot}/public/sprites"
        ],
        "inputs": [
          "{projectRoot}/assets/**/*.svg",
          "shared",
          "config"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "test": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
