{
  "name": "example-figma-simple-export",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@neodx/figma": "workspace:*"
  },
  "scripts": {
    "export": "figma export"
  },
  "nx": {
    "targets": {
      "export": {
        "outputs": [
          "assets/icons"
        ],
        "inputs": [
          "{projectRoot}/assets/**/*.svg",
          "shared",
          "config"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
