{
  "name": "example-svg-magic-with-figma-export",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@neodx/figma": "workspace:*",
    "@neodx/std": "workspace:*",
    "@neodx/svg": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vite-tsconfig-paths": "4.2.3",
    "vitest": "1.1.1"
  },
  "scripts": {
    "export-icons": "figma export --verbose",
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "test": "vitest run",
    "lint": "eslint src",
    "dev": "vite",
    "nuke": "rm -rf dist"
  },
  "nx": {
    "targets": {
      "export-icons": {
        "outputs": [
          "{workspaceRoot}/assets/icons"
        ],
        "inputs": [
          "{projectRoot}/assets/**/*.svg",
          "shared",
          "config"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "build-sprite": {
        "outputs": [
          "{workspaceRoot}/public/sprites"
        ],
        "inputs": [
          "{projectRoot}/assets/**/*.svg",
          "shared",
          "config"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
