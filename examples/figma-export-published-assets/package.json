{
  "name": "example-figma-export-file-assets",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@4.1.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@neodx/figma": "workspace:*"
  },
  "scripts": {
    "export": "figma export"
  },
  "nx": {
    "targets": {
      "export": {
        "outputs": [
          "{workspaceRoot}/assets/icons"
        ],
        "inputs": [
          "{projectRoot}/assets/**/*.svg",
          "shared"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
