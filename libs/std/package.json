{
  "name": "@neodx/std",
  "version": "0.0.2",
  "description": "Standard language helpers",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "source": [
    "src/index.ts",
    "src/*/index.ts"
  ],
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint src",
    "dev": "autobuild --watch"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/node": "18.11.18",
    "eslint": "8.33.0",
    "eslint-kit": "6.11.0",
    "typescript": "4.9.5"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./array": {
      "import": "./dist/array/index.mjs",
      "require": "./dist/array/index.cjs",
      "types": "./dist/array/index.d.ts"
    },
    "./async": {
      "import": "./dist/async/index.mjs",
      "require": "./dist/async/index.cjs",
      "types": "./dist/async/index.d.ts"
    },
    "./object": {
      "import": "./dist/object/index.mjs",
      "require": "./dist/object/index.cjs",
      "types": "./dist/object/index.d.ts"
    }
  }
}
