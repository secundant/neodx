{
  "name": "@neodx/logger",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "src/index.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint",
    "dev": "autobuild -w"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/node": "18.11.18",
    "@types/tmp": "^0.2.3",
    "eslint": "8.33.0",
    "eslint-kit": "6.11.0",
    "tmp": "0.2.1",
    "typescript": "4.9.5"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  }
}
