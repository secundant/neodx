{
  "name": "@neodx/fs",
  "version": "0.0.2",
  "description": "File system helpers",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "source": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/node": "18.11.18",
    "@types/tmp": "^0.2.3",
    "eslint": "8.33.0",
    "eslint-kit": "6.11.0",
    "tmp": "0.2.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@neodx/std": "*",
    "tiny-glob": "0.2.9"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  }
}
