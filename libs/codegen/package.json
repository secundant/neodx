{
  "name": "@neodx/codegen",
  "private": true,
  "description": "Code generation API",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.7",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "source": "src/index.ts",
  "author": "Dmitry Remezov",
  "license": "MIT",
  "homepage": "https://github.com/secundant/neodx/tree/main/libs/codegen",
  "repository": {
    "type": "git",
    "url": "https://github.com/secundant/neodx.git",
    "directory": "libs/codegen"
  },
  "bugs": {
    "url": "https://github.com/secundant/neodx/issues"
  },
  "keywords": [
    "template",
    "codegen",
    "generator",
    "boilerplate",
    "neodx"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "sideEffects": false,
  "scripts": {
    "add-example": "node scripts/add-example.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint",
    "dev": "autobuild -w"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/ejs": "3.1.3",
    "@types/semver": "7.5.3",
    "@types/tmp": "0.2.4",
    "tmp": "0.2.1"
  },
  "dependencies": {
    "@neodx/fs": "0.0.10",
    "@neodx/pkg-misc": "0.0.8",
    "@neodx/std": "0.2.0",
    "@neodx/vfs": "0.1.10",
    "ejs": "3.1.9",
    "jsonc-parser": "3.2.0",
    "picocolors": "1.0.0",
    "prettier": "3.0.3",
    "semver": "7.5.4"
  },
  "nx": {
    "targets": {
      "add-example": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
