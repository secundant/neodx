{
  "name": "@neodx/codegen",
  "description": "Code generation API",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "source": "src/index.ts",
  "author": "Dmitry Remezov",
  "license": "MIT",
  "homepage": "https://github.com/secundant/neodx/tree/main/libs/codegen",
  "repository": {
    "type": "git",
    "url": "https://github.com/secundant/neodx.git",
    "directory": "libs/codegen"
  },
  "bugs": {
    "url": "https://github.com/secundant/neodx/issues"
  },
  "keywords": [
    "template",
    "codegen",
    "generator",
    "boilerplate",
    "neodx"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "add-example": "node scripts/add-example.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint",
    "dev": "autobuild -w"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/ejs": "^3.1.1",
    "@types/node": "18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/semver": "^7.3.13",
    "@types/tmp": "^0.2.3",
    "eslint": "8.33.0",
    "eslint-kit": "6.11.0",
    "tmp": "0.2.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@neodx/fs": "*",
    "@neodx/std": "*",
    "ejs": "3.1.8",
    "jsonc-parser": "3.2.0",
    "picocolors": "1.0.0",
    "prettier": "2.8.3",
    "semver": "7.3.8"
  },
  "nx": {
    "targets": {
      "add-example": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
