{
  "name": "@neodx/svg",
  "packageManager": "yarn@3.2.0",
  "version": "0.6.1",
  "description": "Supercharge your icons ⚡️",
  "author": {
    "name": "Dmitry Remezov",
    "email": "mysecundant@gmail.com",
    "url": "https://github.com/secundant"
  },
  "contributors": [
    "secundant"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/secundant/neodx.git",
    "directory": "libs/svg"
  },
  "homepage": "https://github.com/secundant/neodx/tree/main/libs/svg",
  "bugs": "https://github.com/secundant/neodx/issues",
  "keywords": [
    "svg",
    "icons",
    "typescript",
    "sprite",
    "generator",
    "react",
    "svgr"
  ],
  "source": [
    "src/index.ts",
    "src/cli.ts",
    "src/esbuild.ts",
    "src/rollup.ts",
    "src/rspack.ts",
    "src/vite.ts",
    "src/webpack.ts",
    "src/plugins/index.ts"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "sprite": "bin.mjs"
  },
  "files": [
    "package.json",
    "bin.mjs",
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint src"
  },
  "dependencies": {
    "@neodx/fs": "0.0.10",
    "@neodx/log": "0.3.0",
    "@neodx/std": "0.2.0",
    "@neodx/vfs": "0.1.11",
    "chokidar": "3.5.3",
    "colord": "2.9.3",
    "commander": "11.1.0",
    "pathe": "1.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svgo": "3.0.2",
    "svgson": "5.3.1",
    "unplugin": "1.5.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@swc/helpers": "0.5.3",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/svgo": "3.0.0",
    "clsx": "2.0.0",
    "prettier": "3.0.3"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "default": "./dist/esbuild.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "default": "./dist/rollup.mjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "require": "./dist/rspack.cjs",
      "default": "./dist/rspack.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "default": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "default": "./dist/webpack.mjs"
    }
  }
}
