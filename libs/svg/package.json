{
  "name": "@neodx/svg",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.2",
  "description": "Supercharge your icons ⚡️",
  "source": "src/index.ts",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "sprite": "bin.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint src"
  },
  "dependencies": {
    "@neodx/codegen": "workspace:*",
    "@neodx/fs": "workspace:*",
    "@neodx/std": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sade": "1.8.1",
    "svgo": "3.0.2",
    "svgson": "5.2.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@neodx/autobuild": "workspace:*",
    "@swc/helpers": "0.4.14",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/svgo": "^3.0.0",
    "clsx": "1.2.1",
    "eslint": "8.31.0",
    "eslint-kit": "6.10.0",
    "prettier": "2.8.2",
    "typescript": "4.9.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  }
}
