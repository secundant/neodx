{
  "name": "@neodx/log",
  "packageManager": "yarn@3.2.0",
  "description": "A lightweight universal logging framework",
  "version": "0.1.4",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.mjs",
  "source": [
    "src/index.ts",
    "src/node/index.ts",
    "src/utils/index.ts"
  ],
  "keywords": [
    "log",
    "cli",
    "logger",
    "logging",
    "hackable",
    "colorful",
    "console",
    "json",
    "tiny",
    "isomorphic"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "author": {
    "name": "Dmitry Remezov",
    "email": "mysecundant@gmail.com",
    "url": "https://github.com/secundant"
  },
  "contributors": [
    "secundant"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/secundant/neodx.git",
    "directory": "libs/log"
  },
  "homepage": "https://github.com/secundant/neodx/tree/main/libs/log",
  "bugs": "https://github.com/secundant/neodx/issues?q=is%3Aopen+is%3Aissue+label%3Ascope%3Alog",
  "sideEffects": false,
  "scripts": {
    "compare-bundle": "tsx bench/compare-bundle.ts",
    "compare-output": "tsx bench/compare-output.ts",
    "show-output": "tsx bench/neodx-log-output.ts",
    "bench": "vitest bench",
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint"
  },
  "dependencies": {
    "@neodx/colors": "*",
    "@neodx/std": "*"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/bunyan": "^1.8.8",
    "@types/tmp": "^0.2.3",
    "bunyan": "^1.8.15",
    "loglevel": "^1.8.1",
    "pino": "^8.12.0",
    "tmp": "^0.2.1",
    "winston": "^3.8.2"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/node/index.mjs",
        "require": "./dist/node/index.cjs"
      },
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs"
    }
  }
}
