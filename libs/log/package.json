{
  "name": "@neodx/log",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": [
    "src/index.ts",
    "src/node/index.ts"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint",
    "dev": "autobuild -w"
  },
  "dependencies": {
    "@neodx/std": "*"
  },
  "peerDependencies": {
    "@neodx/colors": "*"
  },
  "peerDependenciesMeta": {
    "@neodx/colors": {
      "optional": true
    }
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/bunyan": "^1.8.8",
    "@types/tmp": "^0.2.3",
    "bunyan": "1.8.15",
    "loglevel": "1.8.1",
    "pino": "8.11.0",
    "signale": "1.4.0",
    "tmp": "0.2.1",
    "winston": "3.8.2"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.cjs"
    }
  }
}
