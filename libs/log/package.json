{
  "name": "@neodx/log",
  "packageManager": "yarn@4.1.0",
  "description": "A lightweight universal logging framework",
  "version": "0.4.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.mjs",
  "source": [
    "src/*/index.ts",
    "src/*.ts"
  ],
  "keywords": [
    "log",
    "cli",
    "logger",
    "logging",
    "hackable",
    "colorful",
    "console",
    "json",
    "tiny",
    "isomorphic"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "author": {
    "name": "Dmitry Remezov",
    "email": "mysecundant@gmail.com",
    "url": "https://github.com/secundant"
  },
  "contributors": [
    "secundant"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/secundant/neodx.git",
    "directory": "libs/log"
  },
  "homepage": "https://github.com/secundant/neodx/tree/main/libs/log",
  "bugs": "https://github.com/secundant/neodx/issues",
  "sideEffects": false,
  "scripts": {
    "compare-bundle": "tsx bench/compare-bundle.ts",
    "compare-output": "tsx bench/compare-output.ts",
    "show-output": "tsx examples/big-list-of-logs.ts",
    "bench": "vitest bench",
    "typecheck": "vitest run --typecheck.only",
    "clean": "yarn dlx rimraf dist",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint"
  },
  "dependencies": {
    "@neodx/colors": "0.2.7",
    "pathe": "1.1.2"
  },
  "devDependencies": {
    "@neodx/autobuild": "*",
    "@types/bunyan": "1.8.11",
    "@types/express": "4.17.21",
    "@types/koa": "2.14.0",
    "@types/tmp": "0.2.6",
    "bunyan": "1.8.15",
    "cli-truncate": "3.1.0",
    "express": "4.18.2",
    "koa": "2.14.2",
    "loglevel": "1.8.1",
    "pino": "8.16.1",
    "tmp": "0.2.1",
    "tsx": "4.7.1",
    "winston": "3.11.0"
  },
  "peerDependencies": {
    "@types/express": "*",
    "@types/koa": "*",
    "express": "*",
    "koa": "*"
  },
  "peerDependenciesMeta": {
    "@types/express": {
      "optional": true
    },
    "@types/koa": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "koa": {
      "optional": true
    }
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/node/index.mjs",
        "require": "./dist/node/index.cjs"
      },
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.mjs",
      "require": "./dist/express.cjs"
    },
    "./koa": {
      "types": "./dist/koa.d.ts",
      "import": "./dist/koa.mjs",
      "require": "./dist/koa.cjs"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "import": "./dist/http/index.mjs",
      "require": "./dist/http/index.cjs"
    }
  }
}
