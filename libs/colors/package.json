{
  "name": "@neodx/colors",
  "version": "0.0.1",
  "private": true,
  "description": "Formatting and color utils for the terminal",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "source": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "autobuild",
    "test": "vitest run",
    "lint": "eslint src",
    "nuke": "rm -rf dist"
  },
  "devDependencies": {
    "@neodx/autobuild": "*"
  },
  "dependencies": {
    "@neodx/std": "*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  }
}
