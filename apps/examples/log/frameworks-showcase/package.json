{
  "name": "@neodx/example-log-frameworks-showcase",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@4.1.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "start:koa": "tsx --watch koa/app.ts",
    "start:http": "tsx --watch node-http/app.ts",
    "start:express": "NODE_ENV=production tsx --watch express/app.ts"
  },
  "dependencies": {
    "@neodx/log": "workspace:*",
    "express": "4.19.2",
    "koa": "2.15.3",
    "koa-router": "12.0.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/koa": "2.15.0",
    "@types/koa-router": "7.4.8",
    "tsx": "4.16.2",
    "typescript": "5.4.5"
  }
}
